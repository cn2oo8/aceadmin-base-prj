<br/>



		 <div class="page_operate">
			 <button class="btn  btn-primary" type="button" id="add-acProject">
				<span class="glyphicon glyphicon-plus"></span> 创建 AcProject</button>
		</div>


	<div id="dl_AcProject"></div>
</div>


<script language="javascript">

	function queryAcProjectList(pageNo) {
		if (!pageNo) {
			pageNo = 1;
		}
		var page = pageNo;
		var url = '/test/acProject/list.action';
		var param = {
			page: page
		};
		
		var pageSize=jQuery("#JPageSize_queryList").val();
        if(pageSize && !isNaN(pageSize)){
            param['pageSize']=pageSize;
        }

		gUtils.fMergeParam(param,"search_form_AcProject");
		gUtils.fGetHtml(url, 'dl_AcProject', param,'数据加载中...');
	}
	queryAcProjectList();
	




void function(j) {
	jQuery("#add-acProject").click(function(){
		var url='/test/acProject/add.action';
		url=gUtils.fMakeFullLink(url);
		window.location=url;
	});
	
}(jQuery);

</script>
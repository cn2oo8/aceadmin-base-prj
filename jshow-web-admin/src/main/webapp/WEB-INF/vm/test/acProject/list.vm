
<div class="row-fluid">
	<div class="span12">
		<table class="table table-striped table-bordered table-hover table-condensed">
			<thead>
				<tr>
 			<th><span>id</span></th>
			<th><span>名称</span></th>
			<th><span>项目key</span></th>
			<th><span>类型</span></th>
			<th><span>扩展1</span></th>
			<th><span>扩展2</span></th>
			<th><span>扩展3</span></th>
			<th><span>创建人</span></th>
			<th><span>修改人</span></th>
			<th><span>并发版本号</span></th>
			<th><span>数据版本</span></th>
			<th><span>状态</span></th>
			<th><span>修改时间</span></th>

				<th class="operate">操作</th>
				</tr>
			</thead>


			<tbody>
	#if($list && $list.size()!=0)
		    #foreach($item in $list)
		   <tr>
				 			<td>$!item.id</td>
			<td>$!item.name</td>
			<td>$!item.projectKey</td>
			<td></td>
			<td>$!item.ext1</td>
			<td>$!item.ext2</td>
			<td>$!item.ext3</td>
			<td>$!item.creator</td>
			<td>$!item.operator</td>
			<td>$!item.concurrentVersion</td>
			<td>$!item.dataVersion</td>
			<td></td>
			<td>$!dateFormatUtils.format($!item.modified)</td>

				<td class="operate">
					<div class="visible-md visible-lg hidden-sm hidden-xs btn-group">
						<button class="btn btn-xs btn-success JUpdate-acProject" type="button" itemid="$!item.id"><i class="icon-edit bigger-120"></i>编辑</button>
						<button class="btn btn-xs btn-info JView-acProject" type="button" itemid="$!item.id"><i class="icon-eye-open bigger-120"></i>查看</button>
						<button class="btn btn-xs btn-danger JDelete-acProject" type="button" itemid="$!item.id"> <i class="icon-trash bigger-120"></i>删除</button>
					</div>
				</td>
		  </tr>
			#end
		#else
		<tr>
					<td colspan="16" class="message_default">没有记录！</td>
		</tr>
		#end


			</tbody>


		</table>

		#if($pageQuery)
			#showPageQuery($pageQuery,'queryAcProjectList','input')
		#end

	</div>
</div>




<script language="javascript">
void function(j) {	
	j('.JDelete-acProject').click(function() {
		var id = j(this).attr('itemid');
		var config={title:'删除AcProject',info:'您确定要删除AcProject吗？',"queryList":"queryAcProjectList",
		url: '/test/acProject/doDelete.action',param:'id='+id};
		gUtils.fConfirmAndRefreshList(config);
	});
	
	j('.JUpdate-acProject').click(function() {
		var id = j(this).attr('itemid');
		//gDialog.fCreate({ title: '修改AcProject', url: '/test/acProject/update.action?id='+id,width:700}).show();
		var url='/test/acProject/update.action?id='+id;
		window.location=gUtils.fMakeFullLink(url);
	});

	j('.JView-acProject').click(function() {
		var id = j(this).attr('itemid');
		//gDialog.fCreate({ title: '查看AcProject', url: '/test/acProject/view?id='+id,width:700}).show();
		var url='/test/acProject/view.action?id='+id;
		window.location=gUtils.fMakeFullLink(url);
	});
}(jQuery);
</script>